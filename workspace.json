{
  "version": 1,
  "projects": {
    "netlify-plugin-next-dynamics": {
      "root": "libs/netlify-plugin-next-dynamics",
      "sourceRoot": "libs/netlify-plugin-next-dynamics/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/netlify-plugin-next-dynamics/tsconfig.lib.json",
              "libs/netlify-plugin-next-dynamics/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/netlify-plugin-next-dynamics/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/netlify-plugin-next-dynamics/jest.config.js",
            "tsConfig": "libs/netlify-plugin-next-dynamics/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/netlify-plugin-next-dynamics",
            "tsConfig": "libs/netlify-plugin-next-dynamics/tsconfig.lib.json",
            "packageJson": "libs/netlify-plugin-next-dynamics/package.json",
            "main": "libs/netlify-plugin-next-dynamics/src/index.ts",
            "assets": [
              "libs/netlify-plugin-next-dynamic/*.md",
              "libs/netlify-plugin-next-dynamic/*.yml"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  }
}
